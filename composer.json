{
    "name": "reedware/laravel-sms-twilio",
    "description": "Adds the ability to send text messages using Twilio.",
    "keywords": ["laravel", "sms", "twilio"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tyler Reed",
            "email": "tylernathanreed@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "reedware/laravel-sms": "^0.3.2",
        "twilio/sdk": "^6.10"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "laravel/framework": "^6.0|^7.0|^8.0",
        "orchestra/testbench-core": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.5|^9.5"
    },
    "autoload": {
        "psr-4": {
            "Reedware\\LaravelSMS\\Twilio\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Reedware\\LaravelSMS\\Tests\\Twilio\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Reedware\\LaravelSMS\\Twilio\\TwilioServiceProvider"
            ]
        }
    }
}
